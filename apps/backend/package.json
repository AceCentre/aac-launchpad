  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "nodemon": "^2.0.15",
    "scripts": "*",
    "tsconfig": "*",
    "types": "*",
    "typescript": "^4.5.3"
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "dependencies": {
    "apollo-server": "^3.6.7",
    "apollo-server-core": "^3.6.7",
    "templates": "*"
  }
}
