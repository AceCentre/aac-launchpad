{
  "name": "backend",
  "bin": {
    "launchpad": "dist/index.js"
  },
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon --exec \"tsc && node dist/index.js\" -e .ts -i dist",
